{
    "compilerOptions": {
      "target": "ES6",
      "module": "ESNext",  
      "lib": ["DOM", "ESNext"],
      "allowJs": true,
      "outDir": "./dist",
      "rootDir": "./src",
      "strict": true,
      "noImplicitAny": false,
      "moduleResolution": "Node",
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "jsx": "react-jsx"
    },
    "include": ["src"],
    "exclude": ["node_modules", "dist"]
  }
    